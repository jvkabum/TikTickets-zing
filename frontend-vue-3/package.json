    "lint:fix": "eslint --ext .js,.vue src --ignore-path .eslintignore --fix --quiet",
    "format": "prettier --write \"src/**/*.{js,vue,css,scss}\" && npm run lint:fix",
    "format:check": "prettier --check \"src/**/*.{js,vue,css,scss}\"",
